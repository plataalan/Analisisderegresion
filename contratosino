install.packages("foreign")
require(foreign)
install.packages("questionr")
require(questionr)
file.choose()
contrato<- read.dbf("C:\\Users\\SalaE-15\\Desktop\\sdemt316.dbf")
wtd.table(contrato$TIP_CON, weights = contrato$fac_mti)
contratosino<- subset(contrato, contrato$POS_OCU=='1')
install.packages("plyr")
library(plyr)
contratosino$TIP_CON <- revalue(factor(contratosino$TIP_CON),c("6"=NA)
wtd.table(contratosino$TIP_CON, weights = contratosino$fac_mti)
contratosino$TIP_CON<-factor(contratosino$TIP_CON,levels = c(1,2,3,4,5),labels = c("contrato","temporal","tipo indefinido","contrato diferente","contrato no escrito")) 
require(nnet)
model<-multinom(TIP_CON~1, data = contratosino, trace=FALSE)
summary(model)
modfin <- multinom(TIP_CON~SEX+EDA ,data=contratosino)
g<- summary((modfin, wald=T))
print(g)
install.packages("stargazer")
require(stargazer)
s <- stargazer(modfin, type= 'html', title = "coefficientes", summary = T)
stargazer(modfin, type = "text", title = "mlogit", digits = 2, out="ruta a donde se manda.txt")
