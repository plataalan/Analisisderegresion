install.packages("foreign")
require(foreign)
install.packages("questionr")
require(questionr)
solorural<-data.frame(read.spss("C://Users//Sala-C16//Downloads//solorural.sav"))
wtd.table(solorural$sex, weights = solorural$fac_mti)
wtd.table(solorural$eda, weights = solorural$fac_mti)
ninocu <- subset(solorural, solorural$c_ocu=='ocupado')
####recodificar de otra forma
install.packages("plyr")
library(plyr)
solorural$t_per <- revalue(factor(solorural$t_per),c("0"=NA))
table(solorural$t_per)
#####limpiando base###
ninocu$escol_jefe <- revalue(factor(ninocu$escol_jefe),c("7"=NA))
table(ninocu$escol_jefe)
ninocu$t_per<-factor(ninocu$t_per,levels = c(1,2,3),labels = c("es un familiar","no es un familiar","trabaja solo o por su cuenta"))
wtd.table(ninocu$t_per)
ninocu$asis <- revalue(factor(ninocu$asis),c("3"=NA))
ninocu$asis<-factor(ninocu$asis,levels = c(1,2),labels = c("asiste","no asiste"))
wtd.table(ninocu$asis)
####aplicar regresion
require(nnet)
mod1<-multinom(t_per~1, data = ninocu, trace=FALSE)
summary(mod1)
modfin <- multinom(t_per~sex+eda1+asis+sex_jefe+escol_jefe+t_apo1+ocu_jefe, data=ninocu)
g<- summary((modfin, wald=T))
print(g)
####tabla en latex
install.packages("stargazer")
require(stargazer)
s <- stargazer(modfin, type= 'html', title = "coefficientes", summary = T)
stargazer(modfin, type = "text", title = "mlogit", digits = 2, out="ruta a donde se manda.txt")
